   <script>
	var cartItems 	= 0;
	var addons		= [];
	function ObjectLength( object ) {
		var length = 0;
		for( var key in object ) {
			if( object.hasOwnProperty(key) ) {
				++length;
			}
		}
		return length;
	};
    function calculation() {
        var total = 0
        $(".total").each(function () {
			total += parseFloat($(this).text().replace("Rs. ", ""));
			console.log($(this).text());
        });
		total 		= Math.round(total*100)/100
		console.log(total)
		subTotal   	= total/((13/100)+1);
		subTotal 	= Math.round(subTotal*100)/100
		console.log(subTotal)
		taxAmount  	= Math.round((total-subTotal)*100)/100;
		console.log(taxAmount)
        $("span.subTotal").empty().text(subTotal);
        $("span.taxAmount").empty().text(taxAmount);
        $("span.totalAmount").empty().text((subTotal+taxAmount+50));
        $("input.subTotal").val(subTotal);
        $("input.taxAmount").val(taxAmount);
        $("input.totalAmount").val((subTotal+taxAmount+50));
    }
   	$(document).ready(function () {
   		var baselyneProductImagePath = "https://www.baselyne.io/partnerwebsites/assets"
   		$(".addToCartBtn").click(function (e) {
			e.preventDefault();
   			var _this = $(this);
   			var productid = _this.data("id");
   			var productname = _this.data("name");
   			var productprice = _this.data("price");
   			var productimage = _this.data("image");
   			var productqty = 1;
   			$.ajax({
   				type: "POST",
   				url: "/addtocart.php",
   				data: {
                    "type": "basic",
   					"productid": productid,
   					"productname": productname,
   					"productprice": productprice,
   					"productimage": productimage,
   					"productqty": productqty,
                    "addons" : addons
   				},
   				beforeSend: function () {},
   				success: function (data) {
   					data = JSON.parse(data);
   					console.log(data)
   					$.notify("Product SuccessFully added", 'success');
   					$(".cart-count").empty().append(data["totalcount"]);
   					cartItems = data["cartitems"];
   				}
   			});
   		});
   		$(".cart-dropdown").hover(function () {
   			var cartStr = "";
   			var total = 0;
   			if (cartItems != 0) {
   				$(".dropdown-cart-products").empty();
   				$.each(cartItems, function (id, cartItem) {
   					total += parseInt(cartItem["qty"]) * parseInt(cartItem["price"]);
   					cartStr += `<div class="product" id="product-` + id + `">
									<div class="product-details">
										<h4 class="product-title">
											<a href="javascript:void(0)">` + cartItem["name"] + `</a>
										</h4>
										<span class="cart-product-info">
											<span class="cart-product-qty">` + cartItem["qty"] + `</span>
											x Rs.` + cartItem["price"] + `
										</span>
									</div>
									<figure class="product-image-container">
										<a href="javascript:void(0)" class="product-image">
											<img src="` + baselyneProductImagePath + `/products/` + cartItem["image"] + `" alt="product">
										</a>
										<a href="javascript:void(0)" class="btn-remove deleteCart" data-price="` + cartItem["price"] + `" data-id="` + id + `" title="Remove Product"><i class="icon-cancel"></i></a>
									</figure>
								</div>`;
   				});
   				$(".dropdown-cart-products").append(cartStr);
   				$(".dropdownmenu-wrapper .cart-total-price").empty().append("Rs. " + total)
   			}
   		});
   		$(".dropdown-menu").delegate(".deleteCart", "click", function () {
   			var _this = $(this);
   			var productid = _this.data("id");
   			var price = parseInt(_this.data("price"));
   			$.ajax({
   				type: "POST",
   				url: "/addtocart.php",
   				data: {
   					"minusQtyProductId": productid
   				},
   				beforeSend: function () {},
   				success: function (data) {
   					data = JSON.parse(data);
   					cartItems = data["cartitems"];
   					if (data["qty"] === "remove") {
   						_this.closest(".dropdown-menu").find("#product-"+productid).hide();
   					} else {
   						_this.closest(".product").find(".cart-product-qty").empty().text(data["qty"]);
   					}
   					var total = parseInt($(".cart-total-price").text().replace("Rs. ","")) - price;
   					$(".dropdownmenu-wrapper .cart-total-price").empty().append("Rs. " +total)
   					$(".cart-count").empty().append(data["totalcount"]);
   				}
   			});
   		});
   		$(".icon-down-dir").click(function () {
   			var _this 		= $(this);
   			var qty 		= parseInt(_this.closest(".input-group").find(".vertical-quantity").val());
   			var price 		= parseInt(_this.closest(".product-row").find(".price").text().replace("Rs. ", "").replace("/-", ""));
			var productId	= _this.closest(".product-row").data("cartid");
			$.ajax({
   				type: "POST",
   				url: "/addtocart.php",
   				data: {
   					"minusQtyProductId": productId
   				},
   				beforeSend: function () {},
   				success: function (data) {
   					data = JSON.parse(data);
   					cartItems = data["cartitems"];
   					console.log(data);
   					var total = parseInt($(".cart-total-price").text().replace("Rs. ","")) - price;
   					$(".dropdownmenu-wrapper .cart-total-price").empty().append("Rs. " +total)
   					$(".cart-count").empty().append(data["totalcount"]);
					$.notify("Cart SuccessFully Updated", 'success');
   				}
   			});
   			if(qty==0){
				_this.closest(".product-row").remove();
			}
			   _this.closest(".product-row").find(".total").empty().text("Rs. " + (price * qty) + "/-");
			calculation();

   		});
   		$(".icon-up-dir").click(function () {
   			var _this = $(this);
   			var qty 		= parseInt(_this.closest(".input-group").find(".vertical-quantity").val());
   			var price 		= parseInt(_this.closest(".product-row").find(".price").text().replace("Rs. ", "").replace("/-", ""));
			var productId	= _this.closest(".product-row").data("cartid");
   			_this.closest(".product-row").find(".total").empty().text("Rs. " + (price * qty) + "/-");
			calculation();
			$.ajax({
			type: "POST",
			url: "/addtocart.php",
			data: {
				"plusQtyProductId": productId,
				"qty":qty
			},
			beforeSend: function () {},
			success: function (data) {
				data = JSON.parse(data);
				cartItems = data["cartitems"];
				console.log(data);
				$.notify("Cart SuccessFully Updated", 'success');
			}
		});
   		});
		$('.cliked').click(function() {
			$(".cliked").removeClass("liactive");
			$(this).addClass("liactive");
		});
	});

  
   $(window).scroll(function(){
		if ($(this).scrollTop() > 150) {
		$('#cartnone').removeClass('cart-dnone');
		} else {
		$('#cartnone').addClass('cart-dnone');
		}
	});

   </script>
   

